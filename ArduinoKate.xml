      <item> HIGH </item>
      <item> LOW </item>
      <item> OUTPUT </item>
      <item> INPUT </item>
      <item> DEC </item>
      <item> BIN </item>
      <item> HEX </item>
      <item> OCT </item>
      <item> PI </item>
      <item> HALF_PI </item>
      <item> TWO_PI </item>
      <item> LSBFIRST </item>
      <item> MSBFIRST </item>
      <item> CHANGE </item>
      <item> FALLING </item>
      <item> RISING </item>
      <item> DEFAULT </item>
